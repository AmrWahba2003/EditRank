{
  "name": "ER",
  "description": "",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 22.17.1"
  },
  "feathers": {
    "language": "ts",
    "packager": "npm",
    "database": "mongodb",
    "framework": "express",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "json"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "seed:games": " ts-node src/seed/games.seed.ts",
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "ts-node src/services/index.ts",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "npm run mocha",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.34",
    "@feathersjs/authentication": "^4.5.18",
    "@feathersjs/authentication-client": "^5.0.34",
    "@feathersjs/authentication-local": "^4.5.18",
    "@feathersjs/authentication-oauth2": "^1.3.1",
    "@feathersjs/configuration": "^4.5.17",
    "@feathersjs/errors": "^5.0.34",
    "@feathersjs/express": "^4.5.18",
    "@feathersjs/feathers": "^4.5.17",
    "@feathersjs/mongodb": "^5.0.34",
    "@feathersjs/schema": "^5.0.34",
    "@feathersjs/socketio": "^4.5.18",
    "@feathersjs/transport-commons": "^5.0.34",
    "body-parser": "^2.2.0",
    "cloudinary": "^2.7.0",
    "compression": "^1.8.1",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.16.0",
    "mongoose": "^8.18.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "^5.0.34",
    "@feathersjs/rest-client": "^5.0.34",
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.3",
    "@types/mocha": "^10.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.18.1",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.21",
    "axios": "^1.9.0",
    "cross-env": "^10.0.0",
    "mocha": "^11.2.2",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "tailwindcss": "^4.1.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  }
